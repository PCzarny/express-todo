  ],
  "author": "Piotr Czarny",
  "license": "ISC",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:concepts && npm run test:api",
    "test:concepts": "mocha --recursive \"./src/concepts/**/tests/*.test.js\" --require ./tests/init.js --exit",
    "test:api": "mocha --recursive ./tests/",
    "test:lint": "eslint ./src --ext .js,.ts",
    "start": "node ./dist/index.js",
    "build": "rm -rf ./dist && tsc && copyfiles -u 1 \"src/**/*.pug\" dist/",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^6.2.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.2"
  }
}
